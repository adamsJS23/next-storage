{
  "name": "next-storage",
  "version": "1.0.0",
  "description": "Landing for a storage company",
  "main": "index.js",
  "scripts": {
    "devserver": "live-server",
    "watch:sass": "node-sass sass/main.scss assets/css/style.css -w",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixser -b 'last 10 version' assets/css/style.comp.css -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamsJS23/next-storage.git"
  },
  "author": "Guebre Adama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adamsJS23/next-storage/issues"
  },
  "homepage": "https://github.com/adamsJS23/next-storage#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  }
}
